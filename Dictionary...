# Write code to Calculate frequency of characters in a string..

s = input("Enter the string: ")
dic = {}

for i in s:
    if i in dic:
        dic[i] =dic[i]+ 1
    else:
        dic[i] = 1

print(dic)



#Find non-repeating characters in a string

s=input("Enter the string")

dic={}# to track the frequncy 
for i in s:
    if i in dic:
        dic[i]=dic[i]+1
    else:
        dic[i]=1

print(dic)

ans=[]
for i in dic:
    if dic[i]==1:
        ans.append(i)

print(ans)

